{
    "manifest_version": 2,

    "name": "Help Me",
    "description": "Notify friends when you might make a decision you're not proud of.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "*://*/",
        "https://ajax.googleapis.com/"
    ],
     "background": {
    "scripts": ["block.js"]
  }
}
